        <div class="bar" style="--dashbar-height: 50%;">
            <span class="dash_label">Bar 2</span>
            <span class="dash_value">50</span>
        </div>
        <div class="bar" style="--dashbar-height: 90%;">
            <span class="dash_label">Bar 3</span>
            <span class="dash_value">90</span>
        </div>
        <div class="bar" style="--dashbar-height: 30%;">
            <span class="dash_label">Bar 4</span>
            <span class="dash_value">30</span>
